/*菜单banner内容css样式*/

/*左边菜单内容样式*/
#sMenu .lMenu {
    width: 200px;
    height: 430px;
    box-sizing: border-box;
    background: #f3f3f3;
}

#sMenu .lMenu li p {
    width: 200px;
    line-height: 30px;
    text-indent: 20px;

}

#sMenu .lMenu li:hover p {
    background: #fff;
    z-index: 110;
    position: relative;
    border: 1px solid #ff6600;
    /*padding-right: 0px;*/
    border-right: none;

}

#sMenu .lMenu li:hover dl {
    display: block;
}

#sMenu .lMenu li:hover p a {
    color: #ff6600;
}

#sMenu .lMenu li p a {
    margin-right: 10px;
}

#sMenu .lMenu li dl {
    width: 580px;
    min-height: 430px;
    padding: 0 30px;
    border: 1px solid #ff6600;
    box-sizing: border-box;
    position: absolute;
    left: 200px;
    top: 0px;
    z-index: 105;
    display: none;
    background: white;

}

#sMenu .lMenu li dl dt:first-child {
    font-size: 12px;
    line-height: 40px;
    color: black;
    border: none;
}

#sMenu .lMenu li dl dt:first-child a {
    margin-right: 30px;
}

#sMenu .lMenu li dl dt:not(first-child) {
    line-height: 30px;
    border-bottom: 2px solid #f3f3f3;
    color: #ff6600;
}

#sMenu .lMenu li dl dd {
    line-height: 24px;
}

#sMenu .lMenu li dl dd a {
    margin-right: 10px;
}

/*右边banner内容样式*/
#sMenu .rBanner {
    width: 1000px;
    height: 430px;
    box-sizing: border-box;
}

/*今日推荐 内容样式*/
#recommend ul {
    width: 1188px;
    height: 500px;
    margin: 0 auto;
    border-left: 2px solid #ff6600;

}

#recommend ul li {
    width: 198px;
    height: 250px;
    border-right: 2px solid #ff6600;
    border-bottom: 2px solid #ff6600;
    float: left;
    box-sizing: border-box;
}

#recommend ul li .rectime {
    position: relative;
    left: -2px;
}

#recommend ul li h3 {

    color: #4d4d4d;
    font-weight: normal;
}
#recommend ul li p{
    line-height: 24px;
}
    /*图书电子书内容样式*/
#book .leftBook {
    height: 480px;
    width: 904px;

}

/*图书电子书选项卡样式*/
#book ul li {
    float: left;
    width: 100px;
    font-size: 18px;
    line-height: 34px;
    margin-top: 10px;
    text-align: center;

}

#book .booktop ul li:hover {
    cursor: pointer;
}

#book .leftBook .newProduct > ul {
    display: none;
}

#book .leftBook .newProduct ul.bookAct {
    display: block;
}

#book .leftBook .newProduct h3 {
    font-weight: normal;
    margin-top: 5px;
}

.bookActive {
    background: #ff6600;
    color: #FFF;
}

/*图书电子书左边内容样式*/
#book .leftBook .bookList {
    width: 242px;
    height: 434px;
    background: #f9f9f9;

}

#book .leftBook .bookList img {
    width: 242px;
    height: 284px;
}

#book .leftBook .bookList ul li {
    width: 80px;
    line-height: 28px;
    text-align: center;
    font-size: 14px;

}

#book .leftBook .newProduct {
    width: 660px;
    height: 434px;
}

#book .leftBook .newProduct ul li {
    border-right: 2px solid #ccc;
    border-bottom: 2px solid #ccc;
    margin: 0;
    text-align: left;

}

#book .leftBook .newProduct {
    border-left: 2px solid #ccc;
}

#book .leftBook .newProduct ul li:first-child {
    width: 330px;
    height: 217px;
    float: left;
    padding-left:0px;
}

#book .leftBook .newProduct ul li:not(first-child) {
    width: 165px;
    height: 217px;
    float: left;
    box-sizing: border-box;
    padding-left: 5px;
}

/*图书电子右边内容样式*/
#book .rightBook {
    height: 434px;
    width: 280px;
    border: 2px solid #e6e6e6
}


#book .rightBook h3 {
    width: 280px;
    line-height: 40px;
    text-align: center;
    font-size: 20px;
    color: #808080;
}

#book .rightBook h4 {
    width: 200px;
    line-height: 27px;
    text-align: center;
    font-size: 16px;
    font-weight: normal;
    color: #4d4d4d;

}
#book .rightBook ul {
    padding-left: 20px;
}

#book .rightBook ul li {
    width: 260px;
    border-bottom: 2px solid #e6e6e6;
    position: relative;
    line-height: 27px;
    text-align: center;

}
#book .rightBook ul li span {
    position: absolute;
    top: 0;
    left: -16px;
    color: red;
}
#book .rightBook li div img {

    float: left;
}
#book .rightBook ul li p {

    height:150px;
    line-height: 22px;
    font-size: 14px;
    color: #404040;

}
.clearaAct {
    display: none;
}
.rightBook ul li div.ggactive {
    display:block;
}
.rightBook ul li div{
    display:none;
}
.rightBook ul li:last-child div{
    border: none;
}

/*服装cloth内容公共样式*/
.ggtop ul li {
    width: 106px;
    float: left;
    font-size: 18px;
    line-height: 34px;
    margin-top: 10px;
    text-align: center;
}

.ggbottom .leftList {
    width: 242px;
    height: 500px;
    background: #f9f9f9;

}

.ggbottom .leftList img {
    width: 244px;
    height: 350px;
}

.ggbottom .leftList ul {
    height: 146px;
    width: 240px;
}

.ggbottom .leftList ul li {
    float: left;
    width: 80px;
    line-height: 38px;
    text-align: center;
    font-size: 14px;
}

.ggbottom .rightPor {
    width: 956px;
    height: 502px;
    box-sizing: border-box;
}

.ggbottom .rightPor .ltPor {
    width: 431px;
    height: 340px;
    line-height: 340px;
    border-right: 2px solid #ccc;
    border-left: 2px solid #ccc;
    border-bottom: 2px solid #ccc;
    box-sizing: border-box;

}

.ggbottom .rightPor .rtPor {
    width: 525px;
    height: 340px;
    line-height: 340px;

    box-sizing: border-box;
}

.ggbottom .rightPor .rtPor ul li {
    width: 175px;
    height: 340px;
    border-right: 2px solid #ccc;
    border-bottom: 2px solid #ccc;
    box-sizing: border-box;
    float: left;
}

.ggbottom .rightPor .rtPor ul li img {
    width: 172px;
    height: 330px;
}

.ggbottom .rightPor .lbPor {

    width: 431px;
    height: 160px;
    line-height: 340px;
    border-right: 2px solid #ccc;
    border-bottom: 2px solid #ccc;
    box-sizing: border-box;
    position: relative;
    overflow: hidden;

}

.ggbottom .rightPor .lbPor img {
    position: absolute;
    width: 430px;
    height: 158px;
    left: -1px;
    top: 0;
}

.ggbottom .rightPor .rbPor ul li {
    width: 175px;
    height: 160px;
    border-right: 2px solid #ccc;
    border-bottom: 2px solid #ccc;
    box-sizing: border-box;
    float: left;
}

.ggbottom .rightPor .rbPor ul li img {
    width: 172px;
    height: 156px;
}

.ggbottom .rightPor {
    display: none;
}

.ggbottom .rightPor.activ {
    display: block;
}

.ggfooter {
    width: 1200px;
    height: 63px;
}

.ggfooter ul li {
    width: 132px;
    float: left;

}

.ggfooter ul li img:hover {
    cursor: pointer;

    margin-top: 2px;
}

/*-家居日用 house*/
#house .houseLeft {
    width: 496px;
    height: 500px;
    border: 2px solid #ccc;
    border-top: none;
    overflow: hidden;
}

#house .houseLeft img {
    width: 496px;
    height: 501px;
}

#house .houseRight {
    width: 700px;
    height: 500px;
}

#house .houseRight ul li {
    float: left;
    width: 175px;
    border-right: 2px solid #ccc;
    border-bottom: 2px solid #ccc;
    box-sizing: border-box;

}

#house .houseRight ul:first-child li {
    height: 342px;
}

#house .houseRight ul:last-child li {
    height: 160px;
}

#house .houseRight ul:last-child li img {
    width: 170px;
    height: 157px;
    padding-left: 2px;
}

/*推广商品promot*/
#promot{position: relative;
    height: 660px;}

#promot .promotTab li {
    float: left;
    width: 20px;
    height: 20px;
    margin-right: 30px;
    margin-top: 10px;
    border-radius: 10px;
    background: #cccccc;
    cursor: pointer;
}

#promot .promotTab .active {
    background: #ff6600;
}
#promot .promotList{
    height: 500px;
    position: absolute;
    left: 0;
    top: 60px;
}
#promot .promotList ul {
    height: 500px;
    width: 1200px;
    padding-left: 15px;
}

#promot .promotList ul li {
    width: 216px;
    height: 300px;
    float: left;
    margin-right: 20px;
    text-align: center;
    padding-top: 20px;
}

#promot .promotList ul li .plbottom {
    width: 150px;
    height: 100px;
    margin: 0 auto;

}

#promot .promotList ul li:hover {
    box-shadow: 4px 4px 4px 2px #cccccc;
    background: #f2f2f2;
    border-radius: 10px;
}

#promot .promotList ul li i {
    margin-right: 5px;
    display: inline-block;
    width: 24px;
    height: 15px;
    background: url("../img/news.jpg") no-repeat -4px -4px;

}

#promot .promotList img{
    width: 148px;
    height: 148px;
}

#promot .promotList {
    display: none;
}

#promot .promotList.cur {
    display: block;
}

/*楼层floorcss样式*/

#floor ul {
    width: 40px;
    height: 200px;
    position: fixed;
    left: 50%;
    margin-left: -650px;
    top: 300px;
}

#floor ul li {
    width: 40px;
    height: 40px;
    line-height: 40px;
    color: #fff;
    padding-left: 40px;
    box-sizing: border-box;
    overflow: hidden;
    text-align: center;
}

#floor ul li.ionc1 {
    background:#f2f2f2 url("../img/floor.png") no-repeat 0px 0px;
}

#floor ul li.ionc2 {
    background:#f2f2f2 url("../img/floor.png") no-repeat 0px -80px;
}

#floor ul li.ionc3 {
    background:#f2f2f2 url("../img/floor.png") no-repeat 0px -160px;
}

#floor ul li.ionc4 {
    background:#f2f2f2 url("../img/floor.png") no-repeat 0px -200px;
}

#floor ul li.ionc5 {
    background:#f2f2f2 url("../img/floor.png") no-repeat 0px -320px;
}

#bgsch {
    width: 100%;
    height: 70px;
    background: rgba(0, 0, 0, 0.5);
    position: fixed;
    top: 0px;
    left: 0px;
    display: none;
}

#bgsch .myF {
    position: absolute;
    top: 50%;
    margin-top: -20px;
    left: 50%;
    margin-left: -175px;
    width: 440px;
    height: 36px;
    line-height: 36px;

}
#bgsch .myF input:first-child{
    width: 390px;
    height: 36px;
    line-height: 36px;
}
